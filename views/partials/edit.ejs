


<% if(currentUser && (currentUser._id == user._id)){%>

   <div class="user-btns">
         <a href="/foods/<%=food.id%>/edit">
           <button id="edit-btn" type="button" name="button">EDIT</button>
         </a>

         <form  action="/foods/<%=food.id%>?_method=DELETE" method="post">
           <input id="delete-btn" type="submit" value="DELETE">
         </form>
   </div>


<% } else { %>
  <a href="/users/"><button type="button" name="button">See other Users</button> </a>
  <% } %>
